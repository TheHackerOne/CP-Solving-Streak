//Count Subset Sum

#include <iostream>
using namespace std;

int countsubsetSum(int a[], int S, int n){
    if(S==0)
        return 1;
    if(n==0)
        return 0;
    
    if(a[n-1]>W)
        return knapsack(a, S, n-1);
        
    if(w[n]<=W) knapsack(a, S-a[n-1], n-1) + knapsack(a, S, n-1))
}

int main() {
	int n, S;
	cin>>n>>S;
	int a[n];
	
	for{(int i=0;i<n;i++){
	    cin>>a[i];
	   
	cout<<countsubsetSum(a, S, n);
	
	return 0;
}
