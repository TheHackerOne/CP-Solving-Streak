#include <iostream>
#include<bits/stdc++.h>
using namespace std;


int d[100][100];

int longestCommonSubsequence(string a, string b, int m, int n){
    if(m == 0 || n == 0)
        return 0;
        
    if(d[m][n] != -1)
        return d[m][n];
    
    if(a[m-1] == b[n-1])
        return d[m][n] = 1 + longestCommonSubsequence(a, b, m-1, n-1);
    else
        return d[m][n] = max( longestCommonSubsequence(a, b, m, n-1), longestCommonSubsequence(a, b, m-1, n) );
}

int main() {
    memset(d, -1, sizeof(d));
    
    string a, b;
    cin>>a>>b;
    
    int m, n;
    
    m = a.length();
    n = b.length();
    
    cout<<longestCommonSubsequence(a, b, m, n);
    
	return 0;
}
