/*
// Definition for a Node.
class Node {
public:
    int val;
    vector<Node*> children;

    Node() {}

    Node(int _val) {
        val = _val;
    }

    Node(int _val, vector<Node*> _children) {
        val = _val;
        children = _children;
    }
};
*/

class Solution {
public:
    vector<vector<int>> levelOrder(Node* root) {
        
        queue<Node*> q;
        q.push(root);
        vector<vector<int>> ans;
        if(root == nullptr) return ans;
        while(!q.empty()){
            int size = q.size();
            vector<int> res;
            while(size--){
                Node *node = q.front();
                q.pop();
                res.push_back(node->val);
                for(auto child:node->children){
                    q.push(child);
                }
            }
            ans.push_back(res);
        }
        return ans;
    }
};