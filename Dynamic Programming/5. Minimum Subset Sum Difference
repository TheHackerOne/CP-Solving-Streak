
#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int inRange[100];

bool subsetSum(int a[], int S, int n){
    if(S==0)
        return true;
    if(n==0)
        return false;
    
    if(a[n-1]>S)
        return subsetSum(a, S, n-1);
        
    if(a[n-1]<=S) subsetSum(a, S-a[n-1], n-1) || subsetSum(a, S, n-1);
}

int minsubsetSumdiff(int a[], int sum, int n){
    memset(inRange, -1, sizeof(inRange));
	inRange[0] = 0;   
    for(int i=1;i<=sum/2;i++){
        bool isTrue = subsetSum(a, i, n);
        if(isTrue)
            inRange[i] = 0;
    }
    for(int i=sum/2; i>=0 ;i--)
        if(inRange[i] == 0)
            return i;
}

int main() {
	int n, sum = 0;
	cin>>n;
	int a[n];
	
	for(int i=0;i<n;i++){
	    cin>>a[i];
	    sum += a[i];
	}
	   
	cout<<sum - (2*minsubsetSumdiff(a, sum, n));
	
	return 0;
}
