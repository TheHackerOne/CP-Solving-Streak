#include<bits/stdc++.h>
using namespace std;
int main(){
    string str;
    cin>>str;

    long long num = 0;
    int counter = 0;
    for(int i=0;i<str.length();i++){
        if(str[i] == '1'){
            num += (pow(2, counter));
        }
        counter++;
    }

    if(num%3) cout<<"false";
    else cout<<"true";
    
}