#include<bits/stdc++.h>
using namespace std;

int solution(vector<int> & arr){
    int mid;
        int n = arr.size();
        if(arr[0] < arr[n-1]) return arr[0];
        int lo = 0, hi = n-1;
        while(lo <= hi){
            mid = (lo+hi)/2;
            if(mid != 0 and mid != arr.size()-1 and arr[mid] < arr[mid-1] and arr[mid] < arr[mid+1]){
                return arr[mid];
            }else if(arr[mid] < arr[0]){
                hi = mid-1;
            }else{
                lo = mid+1;
            }
        }
        return arr[mid];
}

int main(){
    int n;
    cin>>n;
    vector<int> arr(n,0);
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    int ans = solution(arr);
    cout<<ans<<endl;
}