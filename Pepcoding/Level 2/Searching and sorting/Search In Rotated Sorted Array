#include<bits/stdc++.h>
using namespace std;

int solution(vector<int> & arr, int target){
   int lo = 0, hi = arr.size()-1;
        int ans = -1;
        
        while(lo <= hi){
            int mid = (lo+hi)/2;
            if(arr[mid] == target){
                return mid;
            }else if(arr[lo] <= arr[mid]){
                if(target >= arr[lo] and target <= arr[mid]){
                    hi = mid-1;
                }else{
                    lo = mid+1;
                }
            }else{
                if(target >= arr[mid] and target <= arr[hi]){
                    lo = mid+1;
                }else{
                    hi = mid-1;
                }
            }
        }
        
        return ans;
}

int main(){
    int n;
    cin>>n;
    vector<int> arr(n,0);
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    int tar;
    cin>>tar;
    int ans = solution(arr,tar);
    cout<<ans<<endl;
}