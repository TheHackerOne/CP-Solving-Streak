#include<bits/stdc++.h>
using namespace std;

int upperBound(vector<int> &arr, int target){
  int ans = -1;

  int lo = 0, hi = arr.size()-1;
  while(lo <= hi){
    int mid = (lo+hi)/2;
    if(arr[mid] == target){
      ans = mid;
      lo = mid+1;
    }else if(arr[mid] < target){
      lo = mid+1;
    }else hi = mid-1;
  }

  return ans;
}

int lowerBound(vector<int> &arr, int target){
  int ans = -1;

  int lo = 0, hi = arr.size()-1;
  while(lo <= hi){
    int mid = (lo+hi)/2;
    if(arr[mid] == target){
      ans = mid;
      hi = mid-1;
    }else if(arr[mid] < target){
      lo = mid+1;
    }else hi = mid-1;
  }

  return ans;
}

void solution(vector<int>& arr, int target, vector<int>& ans){
  int lower = lowerBound(arr, target);
  int upper = upperBound(arr, target);
  ans[0] = lower, ans[1] = upper;
}

int main(){
    int n;
    cin>>n;
    vector<int> arr(n,0);
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    int tar;
    cin>>tar;
    vector<int> ans(2, -1);
    solution(arr, tar, ans);
    cout<<ans[0]<<" "<<ans[1];
    
    
    
    
}