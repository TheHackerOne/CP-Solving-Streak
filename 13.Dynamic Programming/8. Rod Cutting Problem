#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int n, L;
// Rod Cutting Problem

int rodCutting(int price[], int L, int n){
    if(L == 0 || n == 0)
        return 0;
        
    if(n > L)
        return rodCutting(price, L, n-1);
    else if(n <= L)
        return max(rodCutting(price, L, n-1), price[n-1] + rodCutting(price, L-n, n));
        
}

int main() {
	cin>>n>>L;
	int price[n];
	for(int i=0;i<n;i++)
	    cin>>price[i];
	    
	cout<<rodCutting(price, L, n);    
	return 0;
}
